{
    "root": true,
    "extends": [
        "eslint:recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module"
    },
    "env": {
        "browser": true
    },
    "rules": {
        "no-unexpected-multiline": "off"
    },
    "overrides": [
        {
            "env": {
                "node": true
            }
        }
    ]
}
